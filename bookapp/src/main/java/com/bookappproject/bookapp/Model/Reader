public class Reader {
    private int reader_id;
    private String name;
    private int age;
    private String favorite_book;
    private String favorite_genre;

    // constructors
    public Reader(int reader_id, String name, int age){
        reader_id = reader_id;
        name = name;
        age = age;
    }

    public Reader(int reader_id, String name, int age, String favorite_book, String favorite_genre) {
        reader_id = reader_id;
        name = name;
        age = age;
        favorite_book = favorite_book;
        favorite_genre = favorite_genre;
    }



    // getters and setters 

    public int getReaderId() {
        return reader_id;
    }

    public String getName() {
        return name;
    }

    public int getAge() {
        return age;
    }

    public String getFavoriteBook(){
        return favorite_book;
    }

    public String getFavoriteGenre(){
        return favorite_genre;
    }

    public void setReaderId(int reader_id) {
        reader_id = reader_id;
    }

    public void setName(String name) {
        name = name;
    }

    public void setAge(int age) {
        age = age;
    }

    public void setFavoriteBook(String favorite_book){
        favorite_book = favorite_book;
    }

    public void setFavoriteGenre(String favorite_genre){
        favorite_genre = favorite_genre
    }
}